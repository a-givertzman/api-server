[Unit]
Description=API Server wrapping databases, executable and python scripts plugins
After=syslog.target network.target multi-user.target

[Service]
User=scada
Type=simple
Restart=always
RestartSec=5
# WorkingDirectory=/home/scada/api-server/
ExecStart=
ExecStart=api-server --config /home/scada/api-server/config.yaml

[Install]
WantedBy=multi-user.target
